fn may_fail(flag: Bool) -> Result[Int, String] {
  if flag { Ok(41) } else { Err("boom") }
}

fn compute(flag: Bool) -> Result[Int, String] {
  let v := may_fail(flag)?
  Ok(v + 1)
}

fn main() -> Int {
  let good := compute(true)
  let bad := compute(false)

  match good {
    Ok(v) => print(v),
    Err(e) => print(e),
  }

  match bad {
    Ok(v) => print(v),
    Err(e) => print(e),
  }

  0
}
